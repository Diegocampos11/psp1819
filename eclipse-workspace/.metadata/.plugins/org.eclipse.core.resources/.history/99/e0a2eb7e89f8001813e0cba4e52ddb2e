
public class Main {

	public static void main(String[] args) {
		NumInDes num = new NumInDes(1);
		Thread th1 = new Thread( new Runnable() {			
			@Override
			public void run() {
				for (int i = 0; i < 1000; i++) {
					num.setNum( num.getNum() + 1 );
				}
			}
		} );
		th1.start();
		Thread th2 = new Thread( new Runnable() {			
			@Override
			public void run() {
				for (int i = 0; i < 1000; i++) {
					num.setNum( num.getNum() - 1 );
				}
			}
		} );
		th2.start();
		
	}

}
